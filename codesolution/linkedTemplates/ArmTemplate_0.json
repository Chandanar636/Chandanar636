{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "codesolution"
		},
		"LA_Blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LA_Blobstorage'"
		},
		"LS_SQLSever_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqladftraing.database.windows.net"
		},
		"LS_SQLSever_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLADFTraining"
		},
		"LS_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqltraining2.database.windows.net"
		},
		"LS_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLDATABASE"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
