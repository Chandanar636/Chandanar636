{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "codesolution"
		},
		"LA_Blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LA_Blobstorage'"
		},
		"LS_SQLSever_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqladftraing.database.windows.net"
		},
		"LS_SQLSever_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLADFTraining"
		},
		"LS_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqltraining2.database.windows.net"
		},
		"LS_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLDATABASE"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "codesolution_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LA_Blobstorage_connectionString": {
						"value": "[parameters('LA_Blobstorage_connectionString')]"
					},
					"LS_SQLSever_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLSever_properties_typeProperties_server')]"
					},
					"LS_SQLSever_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLSever_properties_typeProperties_database')]"
					},
					"LS_sql_properties_typeProperties_server": {
						"value": "[parameters('LS_sql_properties_typeProperties_server')]"
					},
					"LS_sql_properties_typeProperties_database": {
						"value": "[parameters('LS_sql_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "codesolution_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'codesolution_ArmTemplate_0')]"
			]
		}
	]
}